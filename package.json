{
  "name": "react-maid",
  "author": {
    "name": "Darwin Gautalius",
    "email": "darwin.gautalius@live.com"
  },
  "version": "0.0.7",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@material-ui/core": "^4.9.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darwin-gautalius/react-libs.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "maid:build": "rm -rf maid && tsc --p tsconfig.maid.json && node create-package-json.js",
    "maid:publish": "cd maid && npm publish --access public",
    "maid:build-publish": "npm run maid:build && npm run maid:publish",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "sideEffects": false,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}